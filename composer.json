{
  "name": "outl1ne/nova-medialibrary",
  "description": "A Laravel Nova tool for managing media.",
  "keywords": [
    "laravel",
    "nova"
  ],
  "license": "MIT",
  "require": {
    "php": ">=8.0",
    "laravel/nova": "^4.0",
    "outl1ne/nova-translations-loader": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "Outl1ne\\NovaMediaLibrary\\": "src/"
    },
    "files": [
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Outl1ne\\NovaMediaLibrary\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Outl1ne\\NovaMediaLibrary\\NMLServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://nova.laravel.com"
    }
  ]
}
